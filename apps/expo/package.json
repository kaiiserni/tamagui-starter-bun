{
  "name": "expo-app",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "bunx expo start -c",
    "android": "bunx expo run:android",
    "ios": "bunx expo run:ios",
    "ios:d": "bunx expo run:ios -d",
    "eject": "bunx expo eject",
    "dev": "bunx expo start --dev-client",
    "dev:prod": "bunx expo start --no-dev --minify",
    "prebuild": "bunx expo prebuild",
    "build:ios:preview": "eas build --platform ios --profile preview --local",
    "build:android:preview": "bunx eas build --platform android --profile preview --local",
    "build:ios": "bunx eas build --platform ios --profile production --local",
    "build:android": "bunx eas build --platform android --profile production --local",
    "clean": "git clean -xdf .expo node_modules ios android"
  },
  "dependencies": {
    "@my/ui": "workspace:*",
    "@shopify/flash-list": "^1.6.3",
    "app": "workspace:*",
    "expo-crypto": "~12.6.0",
    "expo-image": "~1.5.1",
    "expo-secure-store": "~12.3.1",
    "expo-system-ui": "~2.4.0",
    "expo-web-browser": "~12.5.0",
    "react-native-mmkv": "^2.12.2",
    "@babel/runtime": "^7.24.6",
    "@expo/config-plugins": "~7.9.2",
    "@react-navigation/native": "^6.1.17",
    "babel-plugin-module-resolver": "^5.0.2",
    "burnt": "^0.12.2",
    "expo": "^51.0.8",
    "expo-constants": "~16.0.1",
    "expo-dev-client": "~4.0.14",
    "expo-font": "~12.0.5",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-router": "3.5.14",
    "expo-splash-screen": "0.27.4",
    "expo-status-bar": "~1.12.1",
    "expo-updates": "~0.25.14",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "0.74.1",
    "react-native-gesture-handler": "~2.16.2",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "~3.31.1",
    "react-native-svg": "15.3.0",
    "react-native-web": "~0.19.12"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@expo/metro-config": "~0.18.4",
    "@tamagui/babel-plugin": "^1.99.1",
    "metro-minify-terser": "^0.80.9",
    "typescript": "^5.4.5",
    "ajv": "^8.13.0"
  },
  "resolutions": {
    "metro": "0.76.0",
    "metro-resolver": "0.76.0"
  },
  "overrides": {
    "metro": "0.80.9",
    "metro-resolver": "0.80.9"
  }
}
