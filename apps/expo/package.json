{
  "name": "expo-app",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "bunx expo start -c",
    "android": "bunx expo run:android",
    "ios": "bunx expo run:ios",
    "ios:d": "bunx expo run:ios -d",
    "eject": "bunx expo eject",
    "dev": "bunx expo start --dev-client",
    "dev:prod": "bunx expo start --no-dev --minify",
    "prebuild": "bunx expo prebuild",
    "build:ios:preview": "eas build --platform ios --profile preview --local",
    "build:android:preview": "bunx eas build --platform android --profile preview --local",
    "build:ios": "bunx eas build --platform ios --profile production --local",
    "build:android": "bunx eas build --platform android --profile production --local",
    "clean": "git clean -xdf .expo node_modules ios android"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@expo/config-plugins": "~7.2.2",
    "@my/ui": "workspace:*",
    "@react-navigation/native": "^6.1.6",
    "@shopify/flash-list": "^1.6.3",
    "app": "workspace:*",
    "babel-plugin-module-resolver": "^4.1.0",
    "burnt": "^0.12.1",
    "expo": "^49.0.0",
    "expo-constants": "~14.4.2",
    "expo-crypto": "~12.6.0",
    "expo-dev-client": "~2.4.12",
    "expo-font": "~11.4.0",
    "expo-image": "~1.5.1",
    "expo-linear-gradient": "~12.3.0",
    "expo-linking": "~5.0.2",
    "expo-router": "^2.0.0",
    "expo-secure-store": "~12.3.1",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "expo-updates": "~0.18.18",
    "expo-web-browser": "~12.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-mmkv": "^2.12.2",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@expo/metro-config": "~0.10.0",
    "@tamagui/babel-plugin": "^1.99.1",
    "metro-minify-terser": "^0.74.1",
    "typescript": "^5.3.3",
    "ajv": "^8.13.0"
  },
  "resolutions": {
    "metro": "0.76.0",
    "metro-resolver": "0.76.0"
  },
  "overrides": {
    "metro": "0.76.0",
    "metro-resolver": "0.76.0"
  }
}
